{
    "compilerOptions": {
        "target": "ES6",
        "downlevelIteration": true,
        "module": "ESNext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "composite": true,
        "declaration": true,
        "outDir": "dist"
    }
}
